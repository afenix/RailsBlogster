<h2><%= @post.title %></h2> 
<blockquote><%= @post.body %></blockquote> 
<cite>~<%= @post.author %></cite> 
<h6>Created on <%= @post.created_at.strftime("%b %d, %Y") %></h6>


<%= link_to "Back", posts_path, :class => "btn btn-primary btn-sm"%>

<%= link_to "Edit", edit_post_path(@post), :class => "btn btn-warning btn-sm"%>

